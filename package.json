{
  "name": "vue3-notification",
  "version": "0.0.5",
  "type": "module",
  "main": "./dist/vue3-notification.umd.js",
  "module": "./dist/vue3-notification.es.js",
  "types": "./dist/index.d.ts",
  "files": [
    "dist"
  ],
  "exports": {
    ".": {
      "import": "./dist/vue3-notification.es.js",
      "require": "./dist/vue3-notification.umd.js",
      "types": "./dist/index.d.ts"
    },
    "./style": "./dist/style.css"
  },
  "scripts": {
    "build": "vue-tsc --noEmit && vite build",
    "build-lib": "vite build -c vite.config.ts"
  },
  "dependencies": {
    "mitt": "3.0.0",
    "vue": "^3.5.21"
  },
  "devDependencies": {
    "@types/node": "^16.10.1",
    "@vitejs/plugin-vue": "^1.10.2",
    "sass": "^1.42.1",
    "typescript": "^4.4.3",
    "vite": "^2.9.18",
    "vite-plugin-dts": "^4.5.4",
    "vite-plugin-libcss": "^1.1.1",
    "vue-tsc": "^0.3.0"
  },
  "author": {
    "name": "Anil Shrestha"
  },
  "homepage": "https://github.com/anilshr25/vue3-notify",
  "license": "MIT",
  "keywords": [
    "vue3",
    "vuejs 3",
    "vue 3",
    "notification",
    "toast",
    "toasts",
    "toaster",
    "promt",
    "async",
    "confirmation",
    "notify",
    "notie",
    "notification-center",
    "snotify",
    "vue3-snotify",
    "vue3-notification",
    "vuejs notifications",
    "vue3 notifications",
    "vue3 toaster"
  ],
  "repository": {
    "type": "git",
    "homepage": "https://github.com/anilshr25/vue3-notify"
  },
  "pnpm": {
    "onlyBuiltDependencies": [
      "@parcel/watcher"
    ]
  }
